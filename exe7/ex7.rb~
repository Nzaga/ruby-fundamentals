#prints out the string provided
puts "Mary had a little lamb."

#prints out the string provided the variable in {} is a string
puts "its fleece was white as #{'snow'}."

#prints out the string provided
puts "And everywhere that Mary went."

#prints out the string that has been multiplied by 10
puts "." * 10 # what`d that do?

#assign C to end1
end1 = "C"

#assign h to end2
end2 = "h"

#assign e to end3
end3 = "e"

#assign e to end4
end4 = "e"

#assign s to end5
end5 = "s"

#assign e to end6
end6 = "e"

#assign B to end7
end7 = "B"

#assign u to end8
end8 = "u"

#assign r to end9
end9 = "r"

#assign g to end10
end10 = "g"

#assign e to end11
end11 = "e"

#assign r to end12
end12 = "r"

#watch that print vs. puts on this line what`s it do?
print end1 + end2 + end3 + end4 + end5 + end6
